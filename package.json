{
  "name": "rpi-node-server",
  "version": "1.0.0",
  "description": "Simple file server for raspberry pi",
  "main": "app/index.js",
  "scripts": {
    "start": "nodemon ./bin/dev",
    "clean": "rm -rf dist",
    "build": "npm run clean && mkdir dist && babel app -s -d dist",
    "production": "npm run build && node ./bin/production"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicwands/rpi-node-server.git"
  },
  "author": "nicwands",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nicwands/rpi-node-server/issues"
  },
  "homepage": "https://github.com/nicwands/rpi-node-server#readme",
  "dependencies": {
    "app-root-path": "^3.0.0",
    "bcrypt": "^4.0.1",
    "body-parser": "^1.19.0",
    "clamscan": "^1.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "css-file-icons": "0.0.7",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.1.6",
    "express-rate-limit": "^5.1.1",
    "get-folder-size": "^2.0.1",
    "helmet": "^3.22.0",
    "http-errors": "latest",
    "image-thumbnail": "^1.0.7",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "path": "^0.12.7",
    "pug": "^2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.2",
    "@babel/register": "^7.6.2",
    "babel-core": "^7.0.0-bridge.0",
    "nodemon": "^2.0.2"
  }
}
